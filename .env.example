# Copy this file to .env and fill in the values
# MongoDB connection string (required)
MONGO_URI=mongodb+srv://<username>:<password>@cluster0.example.mongodb.net/workoutdb?retryWrites=true&w=majority

# Required: Secret key for JWT token generation (minimum 32 characters recommended)
SECRET=your_very_long_secret_key_here_minimum_32_chars

# Optional: port to run the server on
PORT=4000
